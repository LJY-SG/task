      </Label>
      <TextField fx:id="name" layoutX="293.0" layoutY="106.0" />
      <Label layoutX="124.0" layoutY="291.0" prefHeight="53.0" prefWidth="175.0" text="请输入考试科目:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="124.0" layoutY="220.0" prefHeight="53.0" prefWidth="175.0" text="请输入考场:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="124.0" layoutY="157.0" prefHeight="53.0" prefWidth="175.0" text="请输入考试时间:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="subject" layoutX="293.0" layoutY="303.0" />
      <TextField fx:id="address" layoutX="293.0" layoutY="232.0" />
      <TextField fx:id="time" layoutX="293.0" layoutY="169.0" />
