<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ExamController">
   <children>
      <TableView fx:id="table" layoutX="72.0" layoutY="43.0" prefHeight="530.0" prefWidth="831.0">
        <columns>
          <TableColumn fx:id="time" prefWidth="191.0" text="考试时间" />
          <TableColumn fx:id="name" prefWidth="151.0" text="姓名" />
            <TableColumn fx:id="address" prefWidth="139.0" text="考场" />
            <TableColumn fx:id="subject" prefWidth="139.0" text="学科" />
            <TableColumn fx:id="PaperName" prefWidth="210.0" text="试卷名称" />
        </columns>
      </TableView>
      <Button fx:id="InsertExam" layoutX="72.0" layoutY="647.0" mnemonicParsing="false" onAction="#InsertExamOnAction" prefHeight="65.0" prefWidth="134.0" text="添加">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="DeleteExam" layoutX="545.0" layoutY="647.0" mnemonicParsing="false" onAction="#DeleteExamOnAction" prefHeight="65.0" prefWidth="134.0" text="删除">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="ModifyExam" layoutX="298.0" layoutY="647.0" mnemonicParsing="false" onAction="#ModifyExamOnAction" prefHeight="65.0" prefWidth="134.0" text="修改">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="Return" layoutX="769.0" layoutY="647.0" mnemonicParsing="false" onAction="#ReturnOnAction" prefHeight="65.0" prefWidth="134.0" text="返回">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
