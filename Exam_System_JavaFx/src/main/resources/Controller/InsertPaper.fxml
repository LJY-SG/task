<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="305.0" prefWidth="564.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.InsertPaperController">
   <children>
      <Label layoutX="96.0" layoutY="121.0" prefHeight="42.0" prefWidth="139.0" text="请输入试卷名称:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="paperName" layoutX="248.0" layoutY="127.0" />
      <Button fx:id="InsertPaper" layoutX="150.0" layoutY="213.0" mnemonicParsing="false" onAction="#InsertPaperOnAction" prefHeight="42.0" prefWidth="85.0" text="确定" />
      <Button fx:id="Return" layoutX="326.0" layoutY="213.0" mnemonicParsing="false" onAction="#ReturnOnAction" prefHeight="42.0" prefWidth="85.0" text="退出" />
      <Label fx:id="success" alignment="CENTER" layoutX="224.0" layoutY="161.0" prefHeight="30.0" prefWidth="102.0" text="添加成功!" textFill="#f50808" visible="false">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="96.0" layoutY="70.0" prefHeight="42.0" prefWidth="139.0" text="请输入科目:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="subject" layoutX="248.0" layoutY="76.0" />
      <Label fx:id="fail" layoutX="248.0" layoutY="170.0" prefHeight="42.0" prefWidth="139.0" text="添加失败!" textFill="#fc0909" visible="false">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
