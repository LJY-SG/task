<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="494.0" prefWidth="741.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.examination_system.www.controller.Student.ExamInfoController">
   <children>
      <TableView fx:id="table" layoutY="67.0" prefHeight="319.0" prefWidth="741.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="学号" />
          <TableColumn fx:id="name" prefWidth="108.0" text="姓名" />
            <TableColumn fx:id="time" prefWidth="162.0" text="考试时间" />
            <TableColumn fx:id="address" prefWidth="125.0" text="考场" />
            <TableColumn fx:id="subject" prefWidth="75.0" text="考试科目" />
            <TableColumn fx:id="paperName" prefWidth="119.0" text="考卷" />
            <TableColumn fx:id="status" prefWidth="76.0" text="状态" />
        </columns>
      </TableView>
      <Label layoutX="324.0" layoutY="14.0" prefHeight="42.0" prefWidth="184.0" text="考试信息">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="Begin" layoutX="55.0" layoutY="427.0" mnemonicParsing="false" onAction="#BeginOnAction" prefHeight="32.0" prefWidth="112.0" text="开始考试">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="Exit" layoutX="521.0" layoutY="427.0" mnemonicParsing="false" onAction="#ExitOnAction" prefHeight="32.0" prefWidth="112.0" text="退出">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="Query" layoutX="287.0" layoutY="427.0" mnemonicParsing="false" onAction="#QueryOnAction" prefHeight="32.0" prefWidth="112.0" text="查看">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
